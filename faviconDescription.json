{
	"masterPicture": "src/assets/images/svg/iconlight-fontless.svg",
	"iconsPath": "/assets/images/favicon",
	"design": {
		"ios": {
			"pictureAspect": "noChange",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Jasper Albering"
		},
		"desktopBrowser": {
			"design": "raw"
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#0066ff",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			},
			"appName": "Jasper Albering"
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#0066ff",
			"manifest": {
				"name": "Jasper Albering",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 71.09375,
			"themeColor": "#5bbad5"
		}
	},
	"settings": {
		"compression": 5,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": true,
		"htmlCodeFile": true,
		"usePathAsIs": false
	}
}


