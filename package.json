{
    "name": "jasperro-website",
    "version": "1.0.0",
    "description": "Jasper Albering's personal website, built with eleventy",
    "scripts": {
        "start": "yarn run dev",
        "dev": "yarn run clean & run-p gulp:dev webpack:dev",
        "build": "yarn run clean & cross-env NODE_ENV=production run-s gulp:build eleventy:build webpack:build",
        "gulp:dev": "gulp dev",
        "gulp:build": "gulp build",
        "webpack:dev": "webpack --watch",
        "webpack:build": "webpack",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:build": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist/**"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/jasperro/jasperro-website"
    },
    "author": "Jasper Albering",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-navigation": "^0.2.0",
        "@11ty/eleventy-plugin-rss": "^1.0.9",
        "@babel/core": "^7.12.7",
        "@babel/preset-env": "^7.12.7",
        "@tailwindcss/forms": "^0.3.2",
        "@tailwindcss/typography": "^0.4.0",
        "autoprefixer": "^10.0.2",
        "babel-loader": "^8.2.1",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "cssesc": "^3.0.0",
        "cssnano": "^5.0.1",
        "cssnano-preset-advanced": "^5.0.0",
        "dayjs": "^1.9.6",
        "del-cli": "^3.0.1",
        "eleventy-plugin-local-respimg": "^0.4.0",
        "focus-trap": "^6.2.0",
        "focus-visible": "^5.2.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-postcss": "^9.0.0",
        "gulp-svg-sprite": "^1.5.0",
        "html-minifier": "^4.0.0",
        "lodash": "^4.17.20",
        "markdown-it": "^12.0.2",
        "memfs": "^3.1.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.1.9",
        "postcss-import": "^14.0.0",
        "postcss-load-config": "^3.0.0",
        "postcss-loader": "^5.2.0",
        "postcss-nested": "^5.0.1",
        "postcss-preset-env": "^6.7.0",
        "tailwindcss": "^2.1.0",
        "terser-webpack-plugin": "^5.1.1",
        "webpack": "^5.14.0",
        "webpack-cli": "^4.3.1",
        "webpack-manifest-plugin": "^3.0.0"
    },
    "dependencies": {
        "mini-css-extract-plugin": "^1.3.5"
    }
}
